{"code":"import React, { useCallback, useEffect } from 'react';\r\nimport { Toast } from './components';\r\nimport { useToast } from './hooks';\r\nimport { emitter, toastDispatcher } from './utils';\r\nimport { Events } from './types';\r\nimport './ToastContainer.css';\r\nconst ToastContainer = ({ position = 'bottom-left', delay }) => {\r\n    const { toasts, dispatch } = useToast();\r\n    useEffect(() => {\r\n        toastDispatcher({ dispatch, delay });\r\n        return () => {\r\n            emitter.off();\r\n        };\r\n    }, [dispatch, delay]);\r\n    const onClose = useCallback((id) => {\r\n        emitter.emit(Events.HIDE, id);\r\n    }, []);\r\n    return (React.createElement(\"div\", { className: `toastContainer ${position}` }, toasts.map(toast => (React.createElement(Toast, Object.assign({ key: toast.id }, toast, { onClose: onClose }))))));\r\n};\r\nexport default ToastContainer;\r\n//# sourceMappingURL=ToastContainer.js.map","references":["/home/tmdev/HD/Libraries/Toastify-react/toastlib/node_modules/@types/react/index.d.ts","/home/tmdev/HD/Libraries/Toastify-react/toastlib/src/components/index.ts","/home/tmdev/HD/Libraries/Toastify-react/toastlib/src/hooks/index.ts","/home/tmdev/HD/Libraries/Toastify-react/toastlib/src/utils/index.ts","/home/tmdev/HD/Libraries/Toastify-react/toastlib/src/types/index.ts"],"map":"{\"version\":3,\"file\":\"ToastContainer.js\",\"sourceRoot\":\"\",\"sources\":[\"../../../../src/ToastContainer.tsx\"],\"names\":[],\"mappings\":\"AAAA,OAAO,KAAK,EAAE,EAAM,WAAW,EAAE,SAAS,EAAE,MAAM,OAAO,CAAA;AACzD,OAAO,EAAE,KAAK,EAAE,MAAM,cAAc,CAAA;AACpC,OAAO,EAAE,QAAQ,EAAE,MAAM,SAAS,CAAA;AAClC,OAAO,EAAE,OAAO,EAAE,eAAe,EAAE,MAAM,SAAS,CAAA;AAClD,OAAO,EAAE,MAAM,EAAY,MAAM,SAAS,CAAA;AAC1C,OAAO,sBAAsB,CAAA;AAO7B,MAAM,cAAc,GAAwB,CAAC,EAAE,QAAQ,GAAG,aAAa,EAAE,KAAK,EAAE,EAAE,EAAE;IAClF,MAAM,EAAE,MAAM,EAAE,QAAQ,EAAE,GAAG,QAAQ,EAAE,CAAA;IAEvC,SAAS,CAAC,GAAG,EAAE;QACb,eAAe,CAAC,EAAE,QAAQ,EAAE,KAAK,EAAE,CAAC,CAAA;QAEpC,OAAO,GAAG,EAAE;YACV,OAAO,CAAC,GAAG,E